body{ margin:0 auto; font-size:12px; color:#333; font-family:"microsoft yahei" !important; overflow-x:hidden;}
*{ -webkit-box-sizing:border-box; -moz-box-sizing:border-box; box-sizing:border-box;}
span,a,label,b,p,i,em,h1,h2,h3,h4,h5,h6{ display:inline-block; font-style:inherit;}
ul,li,dl,dt,dd,h1,h2,h3,h4,h5{ list-style:none; padding:0px; margin:0px; float:left;}
input[type="text"],input[type="password"]{ text-indent:5px;}
input,select,textarea{ outline:none; color:#666; padding:0px; float:left; font-family:"microsoft yahei" !important;}
img{ border:0px;}
p{ margin:0px; padding:0px;}
a{ text-decoration:none; color:#3c3c3c; cursor:pointer;}
.display_none{ display:none;}
.clear{ clear:both;}


.content{ width:1200px; margin:0 auto; position:relative;}
.content .row{ width:100%; float:left;}
.content .row:after{ content:"";clear:both;}

/***
toper
***/
.toper{ border-bottom:1px solid #e1e1e1; background-color:#f9f9f9; height:29px;}
.toper ul{ float:right;}
.toper ul li{ float:left; line-height:28px; color:#666; position:relative; padding:0 8px;}
.toper ul li a{ color:#666;}
.toper ul li a:hover{ color:#e84d1c; text-decoration:underline;}
.toper ul li i{ margin-right:5px; color:#e84d1c;}
.toper ul li i.jt{ color:#999; margin-left:5px;}
.toper ul li span{ margin:0 2px; font-weight:bold; color:#333;}
.toper ul li a:hover span{ color:#e84d1c; text-decoration:underline;}
.toper ul:first-child{ float:left;}
.toper ul:first-child li{}
.toper ul li.line{ width:1px; height:14px; background-color:#e1e1e1; margin:7px 5px; padding:0px;}
.toper ul li.more_bt{ border-left:1px solid #f9f9f9; border-right:1px solid #f9f9f9;}
.toper ul li.xuan{ background-color:#fff; border-left:1px solid #e1e1e1; border-right:1px solid #e1e1e1; height:30px; position:relative; z-index:9;}
.toper ul li.more_bt .more_cd{ background-color:#fff; min-width:100%; float:left; padding:0 8px; position:absolute; top:29px; right:-1px; left:-1px; border-left:1px solid #e1e1e1; border-right:1px solid #e1e1e1; border-bottom:1px solid #e1e1e1; z-index:9;}
.toper ul li.more_bt .more_cd a{ text-align:center;}
.toper ul li.more_bt .more_cd ul{ width:350px; background-color:#fff; border-right:1px solid #e1e1e1; border-bottom:1px solid #e1e1e1; margin-bottom:-1px;}
.toper ul li.more_bt .more_cd ul li{ padding:5px 10px 0 10px;}

/***
header
***/
.header{ height:100px; padding:20px 0; float:left;}
.header .logo{}
.header .logo a img{ height:60px;}
.header .seach_main{}
.header .seach_input{ height:35px; margin:1px 0 4px 0;}
.header .seach_input input[type="text"]{ margin:0px; height:35px; border:2px solid #e84d1c;}
.header .seach_input a{ margin:0px; height:35px; line-height:30px; text-align:center; padding:0px; border:2px solid #e84d1c; background-color:#e84d1c; color:#fff; font-size:16px; cursor:pointer;}
.header .seach_hot{ height:20px; line-height:20px;}
.header .seach_hot a{ margin-right:10px;}

/***
small_header
***/
.small_header{ width:1000px; margin:0 auto;}
.small_header a{ float:left;}
.small_header a img{ height:60px; margin:20px 0 20px 0;}
.small_header h1{ height:60px; line-height:60px; margin:10px 0 10px 30px; font-weight:100; color:#666; font-size:25px;}

/***
菜单
***/
.nav{ border-bottom:2px solid #e84d1c; height:40px;}
.nav .pro_nav{ height:40px; background-color:#cb3302; min-width:200px; max-width:200px; position:relative;}
.nav .pro_nav i{ line-height:38px; color:#fff; margin:0 15px; font-size:18px; float:right;}
.nav .pro_nav h3{ line-height:40px; color:#fff; font-size:15px; text-indent:15px;}
.pro_nav_main{ position:absolute; top:40px; left:0px; z-index:5;}
.pro_nav_list{ background-color:#e84d1c; padding:1px; min-width:200px; max-width:200px;}
.pro_nav_list dl{ height:31px; line-height:31px; color:#fff; padding:0 10px;}
.pro_nav_list dl dd{ height:31px; overflow:hidden}
.pro_nav_list dl dd a{ color:#fff; margin-left:5px; font-size:14px;}
.pro_nav_list dl dt{ float:right;}
.pro_nav_list dl dt i{ font-size:15px !important; line-height:31px !important; color:#fff !important; margin:0px !important;}
.pro_nav_list dl.xuan{ background-color:#fff; color:#e84d1c; width:101%; position:relative; z-index:1;}
.pro_nav_list dl.xuan dd a{ color:#e84d1c;}
.pro_nav_list dl.xuan dd a:hover{ text-decoration:underline;}
.pro_nav_list dl.xuan dt i{ display:none;}
.pro_nav_case{ padding:25px; height:436px; min-width:800px; max-width:1000px; background-color:#fff; top:40px; left:0px; border:1px solid #f66639; border-top:0px; border-left:0px; z-index:4;}
.pro_nav_case dl{ margin-bottom:15px; background-color:#fff;}
.pro_nav_case dt a{ text-align:right; font-size:14px; color:#333; font-weight:bold;}
.pro_nav_case dd{ border-bottom:1px solid #f1f1f1; padding-bottom:10px;}
.pro_nav_case dd a{ margin:0 10px 5px 0; border-left:1px solid #e4e4e4; padding-left:10px; color:#666;}
.pro_nav_case a:hover{ color:#e84d1c;}
.pro_nav_case ul{ padding:0 0 5px 17px;}
.pro_nav_case ul li{ padding:0 0 1px 1px;}
.pro_nav_case ul li a img{ background-color:#e4e4e4;}
.pro_nav_case ul li img:hover{ background-color:#f1f1f1;}
.nav .title_nav{ height:38px; line-height:38px; min-width:800px;}
.nav .title_nav li{ margin-left:30px;}
.nav .title_nav li a{ font-size:15px; font-weight:bold; color:#000;}
.nav .title_nav li a:hover{ color:#e84d1c;}

/***
底部
***/
.service{ margin-top:30px; border-top:1px solid #e1e1e1;}
.service dl{ border-left:1px solid #eee; margin:40px 0; padding:0 10px 0 40px; position:relative;}
.service dt{ font-size:25px; color:#666;}
.service dt i{ margin-right:10px;}
.service dl:first-child{ border-left:0px;}
.service dd{ margin-top:10px;}
.service dd a{ margin:5px 10px 0 0; float:left; color:#666;}
.service dd a:hover{ color:#e84d1c;}
.service hr{}

.footer{ text-align:center; line-height:50px;}

.fastop{ width:35px; position:fixed; right:20px; bottom:20px; background-color:#f9f9f9; border:1px solid #e1e1e1; border-bottom:0px;}
.fastop dl{ height:35px; border-bottom:1px solid #e1e1e1;}
.fastop dt{ text-align:center; line-height:35px; font-size:16px;}
.fastop dd{ display:none; text-align:center; line-height:14px; height:35px; padding:4px 0; background-color:#e84d1c;}
.fastop dd a{ color:#fff;}
.fastop dl:hover dt{ display:none;}
.fastop dl:hover dd{ display:block;}


/***
面包导航
***/
.breadcrumb{ margin:15px 0;}
.breadcrumb h2 a:hover{ color:#e84d1c;}
.breadcrumb i.jt{ float:left; margin:5px 5px 5px 10px; font-size:18px; color:#ccc;}
.breadcrumb dl{ position:relative; cursor:pointer; margin-left:5px;}
.breadcrumb dt{ padding:3px 10px; margin:2px 0; border:1px dotted #aaa;}
.breadcrumb dd{ position:absolute; top:26px; left:0px; width:300px; background-color:#fff; border:1px solid #e84d1c; z-index:1; display:none; padding:15px 0 0 15px;}
.breadcrumb dl:hover dt{ border:1px solid #e84d1c; background-color:#e84d1c; color:#fff;}
.breadcrumb dl:hover dt label{color:#fff;}
.breadcrumb dl:hover dd{ display:block;}
.breadcrumb dd a{ float:left; margin:0 20px 15px 0;}
.breadcrumb dd a:hover{ color:#e84d1c; text-decoration:underline;}
.breadcrumb span{ float:left; margin-right:5px;}
.breadcrumb label{ float:left; color:#e84d1c; max-width:150px; height:17px; overflow:hidden; margin-right:5px;}

/***
分页
***/
.paging{ margin:20px 0; height:35px; position:relative;}
.paging_main{ width:680px; height:35px; position:absolute; top:0px; left:50%; margin-left:-340px;}
.paging_main a{ float:left; height:35px; line-height:35px; padding:0 15px; min-width:35px; text-align:center; margin-right:10px; border:1px solid #e1e1e1; background-color:#f9f9f9; font-size:14px;}
.paging_main a:hover{ box-shadow:1px 1px 3px #eee;}
.paging_main a.xuan{ border:1px solid #fff; background-color:#fff; color:#e84d1c; font-size:18px;}
.paging_main a.disabled{ background-color:#fff; border:1px solid #eee; color:#aaa;}
.paging_main span{ float:left; padding:0 5px; font-size:20px; color:#999; margin-right:10px;}
.paging_main label{ float:left; line-height:35px;}
.paging_main input[type="text"]{ width:60px; margin:0 10px;}
.paging_main input[type="button"]{ margin:0 10px; padding:5px 20px;}


/***********************按钮样式开始***********************/
.jl_btn{ padding:5px 20px; border:0px; color:#fff; font-size:15px; cursor:pointer; font-family:"microsoft yahei"; margin:10px 0; text-align:center; text-shadow:0 0 1px #999;}
.jl_btn i{ margin-right:5px;}

.btn_white{ background-color:#fff; color:#197ec2; border:1px solid #e1e1e1; text-shadow:0 0 1px #eee; padding:4px 19px;}
.btn_white:hover{ background-color:#f5f5f5;}
.btn_orange{ background-color:#ff8800;}
.btn_orange:hover{ background-color:#e98f11;}
.btn_blue{ background-color:#3896d4;}
.btn_blue:hover{ background-color:#197ec2;}
.btn_red{ background-color:#d45149;}
.btn_red:hover{ background-color:#d22d29;}
.btn_grey{ background-color:#868686;}
.btn_grey:hover{ background-color:#666666;}
.btn_impcolor{ background-color:#000 !important;}
.btn_impcolor:hover{ filter:alpha(opacity=70); -moz-opacity:0.7; opacity:0.7;}

.btn_red_link{ border:1px solid #d22d29; padding:4px 19px; color:#d22d29; text-shadow:none; background-color:#ffeded;}

.btn_readonly{ background-color:#ccc; cursor:auto;}

.btn_radius{ border-radius:3px;}

.btn_long{ width:100% !important;}
.btn_medium{ width:50% !important;}
.btn_shot{ width:20% !important;}

.btn_margin_right{ margin-right:10px;}
.btn_margin_left{ margin-left:10px;}


.btn_lbl{ position:relative; float:left; height:25px; margin:5px 10px; min-width:70px; border-radius:15px; overflow:hidden;}
.btn_lbl i{ width:19px; height:19px; line-height:19px; border-radius:50%; background-color:#fff; margin:3px; color:#ccc;}
.btn_lbl div{ position:absolute; top:0px; width:100%; height:25px; line-height:25px; color:#fff; border-radius:15px; cursor:pointer;}
.btn_lbl span{ float:left; width:40px; text-align:center;}

.btn_lbl .lbl_ture{left:0px; background-color:#3896d4;}
.btn_lbl .lbl_ture i{ float:right; box-shadow:-1px 1px 2px #666;}
.btn_lbl .lbl_ture span{ margin-left:5px;}

.btn_lbl .lbl_false{left:100%; background-color:#999;}
.btn_lbl .lbl_false i{ float:left; box-shadow:1px 1px 2px #666;}
.btn_lbl .lbl_false span{ margin-right:5px;}

/***********************按钮样式结束***********************/


/***********************字体样式开始***********************/
.font_red{ color:#ff0000;}
.font_black{ color:#000000;}
.font_blue{ color:#3896d4;}
.font_orange{ color:#ff8800;}
.font_green{ color:#00cc33;}
.font_grey{ color:#b1b1b1;}
.font_normal{ font-style:normal;}
.font_bold{ font-weight:bold;}

.text_center{ text-align:center;}
.text_left{ text-align:left;}
.text_right{ text-align:right;}
/***********************字体样式结束***********************/


