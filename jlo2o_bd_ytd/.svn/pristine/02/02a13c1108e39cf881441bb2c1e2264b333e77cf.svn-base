<script type="text/javascript">
var gsid=unescape($.getUrlParam("gsid"));
$(document).ready(function(){
	$("#header").load("/customer/sydjt/showDpxx/shopHead.html?ss="+Math.random());
	$("#footer").load("/customer/sydjt/foot.html");
	selectDPInfo(gsid);
})
function selectDPInfo(gsid) {
	var xmlData = [{"gsid":gsid}];
	var url="/UserInfo/selectDPInfo.action?XmlData="+JSON.stringify(xmlData);
	var	returnVal=visitService(url);
	$(returnVal.list).each(function(i,val){
		var ZCXX02="";
		if(val.ZCXX02!=null){
			ZCXX02=val.ZCXX02;
			}
		var ZCXX08="";
		if(val.ZCXX02!=null){
			ZCXX08=val.ZCXX08;
			}
		var ZCXX06="";
		if(val.ZCXX06!=null){
			ZCXX06=val.ZCXX06;
			}
		var ZCXX09="";
		if(val.ZCXX09!=null){
			ZCXX09=val.ZCXX09;
			}
		$("#sj-lxwm").append("<span class='beijing'>"+ZCXX02+"</span>"+
				"<span>地址："+ZCXX08+"</span>"+
				"<span>电话："+ZCXX06+"</span>"+
				"<span>邮箱："+ZCXX09+"</span>"
				);
	});
	
}
</script>
