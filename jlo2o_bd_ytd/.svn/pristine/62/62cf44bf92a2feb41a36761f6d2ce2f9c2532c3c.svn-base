function SELECTmethod(){
	var sppl=$("#sppl").val();
	var prista=$("#prista").val();
	var priend=$("#priend").val();
	var sppx=$("#sppx").val();
	var jiage=$("#jiage").val();
	var xiaoliang=$("#xiaoliang").val();
	var SPNAMELIST=$.getUrlParam("SPXX04");
	
	var searchMap={};
	var searchlist=[];
	searchMap.sppl=sppl;
	searchMap.prista=prista;
	searchMap.priend=priend;
	searchMap.DQXX01=DQXX01;
	searchMap.sppx=sppx;
	searchMap.SPNAMELIST=SPNAMELIST;
	searchMap.jiage=jiage;
	searchMap.xiaoliang=xiaoliang;
	//品牌code拼接
	var pinpailist=[];
	$("#search_filter_pinpai").find("a").each(function(index,o){
		pinpailist.push($(o).attr('id').replace("pd",""));
	});
	if(pinpailist.length>0){
		searchMap["ppList"]=pinpailist;
	}
	
	//车系code拼接
	var chexinglist=[];
	$("#search_filter_chexing").find("a").each(function(index,o){
		chexinglist.push($(o).attr('id').replace("cxd",""));
	});
	
	if(chexinglist.length>0){
		searchMap["cxList"]=chexinglist;
	}
	
	//配件类型拼接
	var fenleilist=[];
	$("#search_filter_feilei").find("a").each(function(index,o){
		if($(o).attr('id').indexOf("f3d")!=-1){
			fenleilist.push($(o).attr('id').replace("f3d",""));
		}
		
	});
	if(fenleilist.length>0){
		searchMap["flList"]=fenleilist;
	}
	var oe=$("#oe").val();
	
    if(oe!=""&&oe!=null){
    	searchMap["oe"]=oe;
    }
	searchlist.push(searchMap);
    
	//加入库存验证 -- 待修改
	//查询总数量
	var sumUrl = "/PCdisplay/jlquery/selectHYSPnotloginPXSUM.action?XmlData="+JSON.stringify(searchlist);
	var rumData=visitService(sumUrl);
	var rowNum=rumData[0].SUM;
	//查询数据
	var url=baseUrl+"/PCdisplay/jlquery/selectHYSPnotloginPX.action?XmlData="+JSON.stringify(searchlist);
	var rData0 = "";
	$.ajax({
	    type: "GET",  //请求方式
	    async: false,   //true表示异步 false表示同步
	    url:encodeURI(url),
	    data:{},
	    success: function(data){
	      if (data != null){
	        try{
	        	var json = jQuery.parseJSON(data);
	        	//展示数据
	            showSPXX(json.data);
	            //返回文件名
	            fileName =  json.fileName;
	            //展示分页
	            if(is_pre=="0"){
	            	$(".listBOX").show();
	        		$(".pro-case li").hide();
	            }else{
	            	$(".listBOX").hide();
	        		$(".pro-case li").show();
	            }
	    		$('.center').kkPages({ 
	    	 	   PagesClass:'li.jl', //需要分页的元素
	    	 	   RowNum:rowNum,
	    	 	   FileName:fileName,
	    	 	   PagesMth:pubJson.PagesMth, //每页显示个数 
	    	 	   PagesNavMth:pubJson.PagesNavMth //显示导航个数
	    	 	});
	            $.getScript("/js/jquery.Paging.js",function(){}) ;
	        }catch(e){
	          return;
	        }
	      }
	    },
    	error:function(XMLHttpRequest, textStatus, errorThrown) {
	    	alert("获取数据失败，状态是："+textStatus+"+"+XMLHttpRequest+"+"+errorThrown);
        }
	});

}
