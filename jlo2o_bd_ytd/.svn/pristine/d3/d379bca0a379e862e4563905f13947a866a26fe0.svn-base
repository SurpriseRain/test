    /* 通过数据构建HTML */
    function dToHtml(obj){
        var tpl = $.trim(obj["tpl"]),
                datas = obj["data"],
                fc =  _.template(tpl),
                rh = fc(datas);
        var target = typeof obj["target"] === "string"?$(obj["target"]):obj["target"];
        target.html(rh);
    }
