function checkPhone(){
	var isMobile=/^(?:13\d|15\d|18\d)\d{5}(\d{3}|\*{3})$/; //手机号码验证规则
	if(!isMobile.test($(".phone").val())){
		alert("请正确填写手机号码,例如:13415764179");
		return false;
	}else{
		return true;
	}
}
function checkEmail(){
		var email= /\w+([-+.]\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*/;
		if(!email.test($(".eml").val())){
			alert("请正确输入邮箱地址");
			return false;
		}else{
			return true;
		}
}

