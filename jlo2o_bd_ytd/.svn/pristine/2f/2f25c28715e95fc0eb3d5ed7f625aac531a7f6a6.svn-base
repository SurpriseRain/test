function selectEmail(){
	var XmlData=[];
	var email = $("#eml").val();
	if (email.length != 0) {
		if (eml == email) {
			return true;
		}else{
			XmlData.email=email;
			var url="/Register/selectEmail.action?email="+email;
			var data = visitService(url);
				if(data.state=="success"){
						return true;
					}else{
						alert("该邮箱已注册!");
						return false;
					}
		}
	}else{
			return false;
	}
}


function selectPhone(){
	var XmlData={};
	var phone = $("#phone").val();
	if (phone.length != 0) {
		if (SJHM == phone) {
			return true;
		}else{
			XmlData["SJHM"]=phone;
			var url="/Register/selectSms.action?XmlData="+JSON.stringify(XmlData);
			var data = visitService(url);
				if(data.flag=="1000"){
						return true;
					}else{
						alert("该手机号已注册!");
						return false;
					}
		 }
	}else{
		 return false;
	}
}
		
		
