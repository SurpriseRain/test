		XmlData1[0]["lxr"]=$(".lxr").val();
		XmlData1[0]["dlxzk_code"]=$("#dlxzk_code").val();
		XmlData1[0]["GSLX"]=GSLX;
		//增加状态:当财务审核不通过修改改后变成3；当业务审核不通过修改后变成6
		var state=$("#ZCGS01").val();
		if($("#ZCGS01").val() == "7"){
			state="3";
		}else if($("#ZCGS01").val() == "5"){
			state="6";
		}
		XmlData1[0]["state"]=state;  
		if (GSLX != 44) {
			XmlData1[0]["ZCXX70"]=$("#ZCXX70").val();
		}
