function setSPZXHtml(data) {
	var str = "";
	if(data.SF == null ){
		str += "<li style='height:30px;line-height:30px;'>"+data.SJ+"</li>";
		return str;
		
	}else{
		str += "<li style='height:30px;line-height:30px;'>"+data.SF+""+data.SJ+"</li>";
		return str;
	}
}
