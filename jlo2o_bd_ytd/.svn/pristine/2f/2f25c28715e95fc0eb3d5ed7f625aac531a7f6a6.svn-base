<script type="text/javascript">
function changefile(id){
	$("#filename"+id).val(id);
}
function cancelfile(id){
	$("#filename"+id).val("");
}
function ok(){
	//2015/12/14 ---新增区域覆盖范围-----------开始-------------
	//初始化数据
 	var MyCookie = $.cookie('userInfo');
	var userCookie = JSON.parse(MyCookie);
	var zcxx01=userCookie.ZCXX01;
	var gslx=userCookie.LX;
	if(gslx == '44'){
		
	}else{
		var json = {"zcxx01":zcxx01};
		var url = "/UserInfo/select_QYFGFW.action?XmlData=" + JSON.stringify(json);
		var returnData=visitService(url);
		var s = returnData.resultList.length;
		if(s == 0){
			alert("请联系管理员添加仓库信息");
			return false;
		}else{
			if(returnData.resultList[0].dqbzm01 == null ){
				var r = confirm('区域辐射范围为空,确认继续更新信息吗？');
				if(r==true){
				}else{
					return false;
				}
				
			}
		}
	}
	//2015/12/14 ---新增区域覆盖范围-----------结束-------------
	
	if(checkNull()){
		var temp = $(".add2").attr("style");
		if(temp=="display: none"){
		 	//拼接地址
		 	var ZCXX07="";  //地址编码
		   	var ZCXX08="";  //中文地址
		   	if($("#provinceQY").val() != "0"){
			  	ZCXX07=$("#provinceQY").val();
			  	ZCXX08=ZCXX08+$("#provinceQY option:selected").text();
		   	}
		   	if($("#cityQY").val() != "0"){
			  	ZCXX07=$("#cityQY").val();
			  	ZCXX08=ZCXX08+$("#cityQY option:selected").text();
		   	}
		   	if($("#countryQY").val() != "0"){
		      	ZCXX07=$("#countryQY").val();
			  	ZCXX08=ZCXX08+$("#countryQY option:selected").text();
		   	}
		  	if($("#streetQY").val() != "0"){
		      	ZCXX07=$("#streetQY").val();
			  	ZCXX08=ZCXX08+$("#streetQY option:selected").text();
		   	}
		   	$("#dqdm").val(ZCXX07);
		   	var Faddress;
		   	var address=ZCXX08+$("#QYdzdetail").val();
		   	$("#address").val(address);
		   	Faddress = $("#address").val();
			}else{
		   	$("#dqdm").val(dqdm);
		   	$("#address").val($(".add").val());
		   	Faddress = $("#address").val();
		}
		if (defaultAddress != Faddress) {
			if(!checkedAddress()){
				return false;
			}
		}
	
		var XmlData1 = $("#form1").formToJson();
		XmlData1[0]["yhmName"]=$(".yhmName").val();
		XmlData1[0]["dpName"]=$(".dpName").val();
		XmlData1[0]["add"]=$(".add").val();
		XmlData1[0]["phone"]=$(".phone").val();
		XmlData1[0]["fplx"]=$(".fplx").val();
		XmlData1[0]["eml"]=$(".eml").val();
		XmlData1[0]["qq"]=$(".qq").val();
		XmlData1[0]["weixin"]=$(".weixin").val();
		XmlData1[0]["yyzzfb_code"]=$("#yyzzfb_code").val();
		XmlData1[0]["zzjg_code"]=$("#zzjg_code").val();
		XmlData1[0]["swdj_code"]=$("#swdj_code").val();
		XmlData1[0]["khyh_code"]=$("#khyh_code").val();
		XmlData1[0]["ybnsr_code"]=$("#ybnsr_code").val();
		XmlData1[0]["GSYHK01"]=$("#GSYHK01").val();
		XmlData1[0]["jskhhmc"]=$(".jskhhmc").val();
		XmlData1[0]["jshkhh"]=$(".jshkhh").val();
		XmlData1[0]["jshkhhzh"]=$(".jshkhhzh").val();
