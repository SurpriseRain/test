				$("#search_filter_chexing").append('<a style="border:#e4e4e4 1px solid;color: #ff3c3c;" onclick="javascript:deleteSelectchexing($(this))" id="cxd'+type_code+'">'+newsObj.MOBILE_MODLE+'<label class="label_size">×</label></a><span id="cxd'+type_code+'_span">&nbsp&nbsp&nbsp&nbsp</span>');

				$("#search_filter_chexing_div").show();
				
				SELECTmethod();
			}else{
				alert("车型品牌已选择");
			}
			var obj1=$("a[name='a_cxpp_select1']");
			var obj2=$("a[name='a_cxpp_select2']");
			for(var i=0;i<obj1.length;i++){
				obj1[i].remove();
				$("#cxpp"+(i+1)+"_select1_span").remove();
			}
			for(var i=0;i<obj2.length;i++){
				obj2[i].remove();
				$("#cxpp"+(i+1)+"_select2_span").remove();
			}
