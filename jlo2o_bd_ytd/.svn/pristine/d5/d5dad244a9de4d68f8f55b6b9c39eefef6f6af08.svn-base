//存储浏览车型cookie
function zj_llcx(modle,modle_id,mobile_modle_id){
	setTimeout(function(){
		var llcx = $.cookie("llcx"),
		len = 0, llcxArr = [],exist = false;
		if(!llcx){
			// 添加
			llcxArr = [{
				"modle_id":modle_id,
				"mmid":mobile_modle_id,
				"modle":modle
			}];
			
			$.cookie("llcx", JSON.stringify(llcxArr), {
				expires : 1, path : '/'
			});
		}else{
			llcxArr = JSON.parse(llcx);
			len = llcxArr.length;
			if(len === 11){
				for(var i = 0; i < len; i++){
					if(llcxArr[i]["modle_id"] == modle_id){
						exist = true;
						return;
					}
				}
				
				if(!exist){
					llcxArr.pop();
					
					llcxArr.unshift({
						"modle_id":modle_id,
						"mmid":mobile_modle_id,
						"modle":modle
					});
					
					$.cookie("llcx", JSON.stringify(llcxArr), {
						expires : 1, path : '/'
					});
				}
			}
			
			for(var i = 0; i < len; i++){
				if(llcxArr[i]["modle_id"] == modle_id){
					exist = true;
					return;
				}
			}
			
			if(!exist){
				llcxArr.unshift({
					"modle_id":modle_id,
					"mmid":mobile_modle_id,
					"modle":modle
				});
				
				$.cookie("llcx", JSON.stringify(llcxArr), {
					expires : 1, path : '/'
				});
			}
		}
	},1000);
}

