function select_list(SPXX01,ZCXX01){
	$("#sycx_list").html("");
   	 var XmlData=[];
   	 var json={};
   	 json.SPXX01=SPXX01;
   	 json.ZCXX01=ZCXX01;
   	 XmlData.push(json);
   	 var url="/QFY/selectW_SPCX.action?XmlData="+JSON.stringify(XmlData);
   	 ajaxQ({
   			"url" : url,//原始url 
   			"callback" : function(returnData){
   				if(returnData!=undefined&&returnData!=""){
   					 var spcxJson=returnData.lists;
   					 if(spcxJson[0]!=undefined){
   					 $(spcxJson).each(function(i,spcxlist){
   						var str="";
   						$(spcxlist.cxlist).each(function(j,cxlist){
   							str+="<div style='float:left;margin:8px;width:46%;'>"+cxlist.TYPE+"</div>"
   							});
   						 $("#sycx_list").append("<li style='margin:20px;'><h2>"+spcxlist.BrandName+"</h2><div style='float:left;width:100%;'>"+str+"</div></li>");
   						 });
   					    $("#sycx_zhezhao").show();
   					    $("#sycx_zhezhao_2").show();
   					}else{
   						$("#sycx").append("<li><h4>通配车系</h4></li>");
   					}
   				}else{
   					alert("操作失败!");
   				
   				}
   			}
   		});
}
