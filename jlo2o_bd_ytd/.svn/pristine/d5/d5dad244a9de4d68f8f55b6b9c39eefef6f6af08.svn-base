	 var str="";
	 if(returnData.cxtpList.length==0){
		 str+='<li style="opacity:1;z-index:1;"><img src="/customer/sydjt/images/mrtp.png"  /></li>'; 
	 }else if(returnData.cxtpList.length==1){
	 str+='<li style="opacity:1;z-index:1;" ><img src="' + pubJson.path_sptp + 'cxtp/' + returnData.cxtpList[0].MOBILE_MODLE_ID+'/'+returnData.cxtpList[0].CXTP+'"></li>';
     }else{
