div[jlid="pictureScan"] { margin:0 auto; font-size:12px; color:#666;width: 352px;}
div[jlid="pictureScan"] ul,li,h2,h3,h4,input{ list-style:none; margin:0px; padding:0px;}
div[jlid="pictureScan"] a{ text-decoration:none;}
div[jlid="pictureScan"] .clera{ clear:both;}

/* Clear Fix */ 
div[jlid="pictureScan"] .clearfix:after{content:".";display:block;height:0;clear:both;visibility:hidden;} 
div[jlid="pictureScan"] .clearfix{display:inline-block;} 
/* Hide from IE Mac */ 
div[jlid="pictureScan"] .clearfix{display:block;} 
/* End hide from IE Mac */ 
/* end of clearfix */ 
/*页面全局结束*/
div[jlid="pictureScan"] .tab{ overflow:hidden; }
div[jlid="pictureScan"] .tab li,.tab a,.tab-item{cursor:pointer;float:left;text-align: center;}
div[jlid="pictureScan"] .m, .mt, .mc, .mb{overflow:hidden;}
div[jlid="pictureScan"] .mt .extra{float: right;}

div[jlid="pictureScan"] .right-extra .mt{height:28px;line-height:28px;padding:0 10px 8px;border-top:1px solid #EED97C;background:url(../images/bg_hotsale.gif) repeat-x 0 -552px;color:#c00;}
div[jlid="pictureScan"] .right-extra .tab{border-bottom:2px solid #BE0000;margin-bottom:10px;}
div[jlid="pictureScan"] .right-extra .tab li{position:relative;height:24px;padding:3px 12px 0;background:url(../images/icon_clubs.gif) #ccc no-repeat right -150px;overflow:hidden;margin-right:3px;line-height:24px;font-size:14px;font-weight:bold;color:#c30;}
div[jlid="pictureScan"] .right-extra .tab span{position:absolute;left:0;top:0;z-index:1;width:10px;height:27px;background:url(../images/icon_clubs.gif) no-repeat 0 -150px;}
div[jlid="pictureScan"] .right-extra .tab a{float:none;color:#c30;}
div[jlid="pictureScan"] .right-extra .tab .curr{background-position:right -178px;color:#fff;}
div[jlid="pictureScan"] .right-extra .tab .curr span{background-position:0 -178px;}
div[jlid="pictureScan"] .right-extra .tab .curr a{color:#fff;}
div[jlid="pictureScan"] .right-extra .mc .extra{padding:5px 10px;border-top:1px solid #F3E6C6;}
div[jlid="pictureScan"] .right-extra .total{float:right;}
div[jlid="pictureScan"] .right-extra .total strong{color:#FF7403;}
div[jlid="pictureScan"] .right-extra .total a:link,.right-extra .total a:visited{color:#005aa0;}
div[jlid="pictureScan"] .right-extra .join a:link,.right-extra .join a:visited{color:#FF7604;}
div[jlid="pictureScan"] .right-extra .norecode{padding:0 10px 10px;}
div[jlid="pictureScan"] .right-extra .iloading{margin-bottom:10px;}


/*图片放大镜样式*/
div[jlid="pictureScan"] .jqzoom{float:left;border:none;position:relative;padding:0px;cursor:pointer;margin:0px;display:block;}
div[jlid="pictureScan"] .zoomdiv{z-index:100;position:absolute;top:0px;left:0px;width:420px;height:420px;background:#ffffff;border:1px solid #CCCCCC;display:none;text-align:center;overflow:hidden;}
div[jlid="pictureScan"] .zoomdiv img{width:800px;}
div[jlid="pictureScan"] .jqZoomPup{z-index:10;visibility:hidden;position:absolute;top:0px;left:0px;width:199.5px;height:212.5px;border:1px solid #aaa;background:#ffffff /*url(../images/zoom.png) 50% center no-repeat*/;opacity: 0.5;-moz-opacity: 0.5;-khtml-opacity: 0.5;filter: alpha(Opacity=50);}

/*图片小图预览列表*/
div[jlid="pictureScan"] .jqzoom img{ width:420px; height:420px;}
div[jlid="pictureScan"] .spec-preview{width:420px;height:420px;border:1px solid #DFDFDF;}
div[jlid="pictureScan"] .spec-scroll{clear:both;margin-top:5px;width:422px;}
div[jlid="pictureScan"] .spec-scroll .prev{float:left;margin-right:4px;}
div[jlid="pictureScan"] .spec-scroll .prev:hover{ background-color:#3061EF; color:#fff;}
div[jlid="pictureScan"] .spec-scroll .next{float:right;}
div[jlid="pictureScan"] .spec-scroll .next:hover{ background-color:#3061EF; color:#fff;}
div[jlid="pictureScan"] .spec-scroll .prev,.spec-scroll .next{display:block;font-family:"宋体";text-align:center;width:10px;height:74px; line-height:74px;border:1px solid #CCC;background:#EBEBEB;cursor:pointer;text-decoration:none;}
div[jlid="pictureScan"] .spec-scroll .items{float:left;position:relative;width:390px;height:75px;overflow:hidden;}
div[jlid="pictureScan"] .spec-scroll .items ul{position:absolute;width:999999px;height:74px; margin-top:1px;}
div[jlid="pictureScan"] .spec-scroll .items ul li{float:left;width:83px;text-align:center; margin-right:5px; cursor:pointer;}
div[jlid="pictureScan"] .spec-scroll .items ul li img{border:1px solid #CCC;padding:2px;width:68px;height:68px;}
div[jlid="pictureScan"] .spec-scroll .items ul li img:hover{border:2px solid #3061EF;padding:1px;}