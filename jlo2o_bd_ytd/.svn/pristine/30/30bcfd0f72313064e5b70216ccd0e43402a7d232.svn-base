#qyheader{ width:100%; height:100px; background-color:#fff;}
#qyheader .head-main{ width:1200px; height:100px; margin:0 auto;}
#qyheader .head-main .main-left{ float:left; width:700px;}
#qyheader .head-main .main-left a.img{ float:left; width:225px; height:60px; float:left; margin:20px 0;}
#qyheader .head-main .main-left a.img img{ border: 0 none; height: 85px;width: 239px; margin-top: -13px;}
#qyheader .head-main .main-left span.line{ float:left; width:1px; height:50px; margin:25px 25px; background-color:#eee;}
#qyheader .head-main .main-left div.dpname{ max-width:420px; height:60px; float:left; margin:20px -15px;}
#qyheader .head-main .main-left div.dpname img.qy_logo{ width:50px; height:50px; float:left; margin:5px;}
#qyheader .head-main .main-left div.dpname h3{ margin:0px; padding:0px; height:33px; line-height:33px; color:#3c3c3c; float:left; width:350px; font-family:"microsoft yahei"; font-size:18px; margin-left:5px;}
#qyheader .head-main .main-left div.dpname .dpxinxi{ width:350px; height:20px; float:left; margin:2px 0 0 5px; position:static;}
#qyheader .head-main .main-left div.dpname .dpxinxi span{ float:left; width:80px; height:20px;}
#qyheader .head-main .main-left div.dpname .dpxinxi span b{ color:#ff8800;}




#qyheader .head-main .main-right{ float:left; height:100px; width:450px;margin-left:50px;}
#qyheader .head-main .main-right input.search{ float:left; border:3px solid #ff6700; width:345px; height:34px; margin-top:30px; background:url(../images/search-ico.png) no-repeat; padding-left:25px; }
#qyheader .head-main .main-right a.seabot{ float:left; width:74px; height:40px; background-color:#ff6700; color:#fff; line-height:40px; text-align:center; font-size:20px; font-weight:bold; cursor:pointer; margin-top:30px;}

#qydz{ width:100%; height:100px; background-color:#f5f5f5; background:url(../images/qydzbg.png) repeat-x;}
#qydz .qzdz-main{ width:1200px; height:100px; margin:0 auto;}
#qydz .qzdz-main a img{ width:1200px; height:100px; border:0px;}


#qynav{ width:100%; height:35px; background-color:#ff6700;}
#qynav .nav-main{ width:1200px; height:35px; margin:0 auto;}
#qynav .nav-main a{ float:left; height:35px; color:#fff; font-size:16px; line-height:35px; width:120px; text-align:center; font-family:"microsoft yahei"}
#qynav .nav-main a.qbsp{ width:150px; text-align:left; padding-left:30px; font-family:"microsoft yahei"; font-weight:bold; background:url(../images/qy_qbsp_ico.png); background-color:#ff6700;}
#qynav .nav-main a.qbsp:hover{ background-position:0 -35px;}
#qynav .nav-main a:hover{ background-color:#ed5a00;}
#qynav .nav-main span{ float:left; height:35px; width:0px; border-right:1px solid #ff6700;}
#qynav .nav-main label{ float:right; height:35px; background:url(../images/qy_nav_ico.png) no-repeat; padding:0 10px 0 30px; font-size:14px; color:#fff; line-height:35px; font-family:"microsoft yahei"; margin-left:10px; cursor:pointer; position:relative;}
#qynav .nav-main label div{ width:111px; height:124px; position:absolute; background-color:#fff; z-index:9999; left:-15px; border:1px solid #ccc; display:none;}
#qynav .nav-main label div img{ width:101px; height:114px; margin:5px;}
#qynav .nav-main label.wzm{ background-position:-0 -35px;}

#qynav .nav-main label:hover{ background-color:#206fd7;}
#qynav .nav-main label:hover div{ display:block;}


#qynav .nav-main ul#inav-flshow{width:180px; position:absolute;z-index:999999;background-color:#ff6900; float:left; margin:34px 0 50px 0;display:none; border:1px solid #ff6700; border-top:0px;}
#qynav .nav-main ul#inav-flshow li{width:180px; height:30px;}
#qynav .nav-main ul#inav-flshow li:hover{ background-color:#f5f5f5;}
#qynav .nav-main ul#inav-flshow li a{display:block;line-height:30px; float:left; width:100%; height:30px; text-align:center; font-size:12px; cursor:pointer; color:#fff;}
#qynav .nav-main ul#inav-flshow li a:hover{ background:#ffa05f; color:#fff;font-weight:600;}

 /* shopNav */
#shopNav {width: 100%;height: 35px;background:#ff6700;}
.shopnav-main{width: 1200px;height: 35px;margin: 0 auto;}
.shopnav-main select{height: 25px;line-height: 25px;margin-top: 5px;float: left;}.shopnav-main ul{width: 600px;height:100%;margin:0 auto;}
.shopnav-main ul li{height: 35px;line-height: 35px;float: left;width: 100px;margin-right: 20px;}
.shopnav-main ul li a{display: block;font-size: 14px;height: 30px;line-height: 30px;width: 100px;text-align: center;margin: 7px 10px 0 12px;background: #fff;border-radius: 4px;color:#000;font-weight:600;}
#banner{ width:100%;height:500px;}
