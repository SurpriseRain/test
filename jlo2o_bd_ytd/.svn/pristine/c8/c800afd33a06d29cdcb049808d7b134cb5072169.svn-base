ul.tuidan{ width:720px; background-color:#fff; float:left; padding:10px 40px;border:1px solid #e4e4e4; margin-top:3px; margin-bottom:20px;}
ul.tuidan li{ float:left; width:100%; margin-bottom:5px;}
ul.tuidan li h3{ float:left; font-family:"microsoft yahei"; color:#6666; font-size:18px; margin-top:20px;}
ul.tuidan li span{ font-family:"microsoft yahei"; font-size:14px; color:#6666; float:left;  text-align:right; line-height:37px;}
ul.tuidan li input.text{ float:left; width:300px; height:35px; border:1px solid #ccc; padding:0 5px; width:200px;}
ul.tuidan li .bt{ float:left; line-height:45px; width:20px; left:150px; color:red; font-size:20px; text-align:center;}
ul.tuidan li select{ float:left; height:37px; border:1px solid #ccc; padding:0 0 0 10px; color:#6666;}
ul.tuidan li input.radio{ float:left; width:15px; height:15px; margin:12px 5px 13px 0; cursor:pointer;}
ul.tuidan li input.radio1{ cursor:default;}
ul.tuidan li label{ float:left; line-height:37px; margin-right:20px; font-family:"microsoft yahei"; font-size:14px; color:#6666; }
ul.tuidan li textarea{ float:left; min-height:100px; max-height:150px; min-width:500px; max-width:600px;  margin-top:10px; border:#CCC 1px solid; font-size:14px; }
ul.tuidan a.ty{ float:left; border:#CCC 1px solid; background:#2f80eb; width:150px; line-height:20px; text-align:center; color:#fff; cursor:pointer; margin-left:40px; padding:5px 10px;}
ul.tuidan a.ty:hover{ background:#00F;}
ul.tuidan li .clearlink {text-align: left;width: 36px;}
ul.tuidan li .photook{text-align: left; width: 369px;height:30px; overflow: hidden;text-overflow:ellipsis;}
.photos .add{background-color: #e1e1e1;float:left;  border: 1px solid #eeeeee; color: #666666;  font-size: 20px; height: 20px;line-height: 20px; margin: 10px;  padding: 0 6px; text-align: center;  width: 20px;}
.addphoto{float: left;height: 25px;border: 1px solid #ccc;width: 405px; margin-top:8px;}
.imgsize{width: 188px;height: 188px;border: 1px solid #ccc;float: left;margin: 3px;}
.imgsizes{width: 590px;max-height: 420px; height:auto; float: left;margin: 3px;}