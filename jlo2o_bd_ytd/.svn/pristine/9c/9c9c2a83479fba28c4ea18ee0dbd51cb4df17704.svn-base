/*提示信息样式开始*/
.bmts .successTs,.yxqts .successTs{margin-top:10px;}
	/*一拖多服务管理开始*/
.qyts .errTs{margin-left:129px;}
.ppts{margin-left:126px;}
	/*一拖多服务管理结束*/
.gsts{margin-top:0px;}
.flts .errTs{margin-top:10px;}
td .errTs,td .successTs,#search .errTs,.qyts{float:left;}
#search .errTs{margin-top:-5px;}
td .successTs{margin-top:10px;}
#main ul.userName li .errTs{padding:0 5px;margin-top:-10px;color:#f00;font-size:12px;font-family:"Microsoft YaHei",arial,"宋体";}
.errTs{display:block;width: auto;line-height:30px;height:30px;cursor: pointer;border: 1px solid #ffbdbe;background: #FFEBEB;color: #f00;padding:0 15px;font-size:12px;font-family:"Microsoft YaHei",arial,"宋体"; text-align:left;margin-left:5px;}
.successTs{display:block;width:auto;line-height:30px;height:auto;cursor: pointer;color:#f00;padding:10px 30px;font-size:12px;font-family:"Microsoft YaHei",arial,"宋体"; text-align:left;background: url(../images/tssuccess.png) no-repeat;background-position:10px 0px;}
ul.dqbd li .successTs{margin-top:5px;}
/*提示信息样式结束*/

/*search开始*/
#search{width: auto;margin-top:20px;position:relative;}
.searchMain{position:relative;}
#txt{width: 320px;height: 30px;border:2px solid #ff8800;line-height:20px;font-size:14px;/* border-right:none; */margin-top: -5px;}
#result{position:absolute;top:25px;left:119px;width:317px;border: 1px solid #ccc;border-top: none;display:none;z-index:99999;background:#fff;box-shadow:1px 1px 5px #ccc;list-style: none;}
#result li{height: 30px;border-bottom: 1px dotted #ccc;line-height: 30px;font-size:14px;}
#result li:hover{background: #eee;cursor: pointer;}
/*search结束*/