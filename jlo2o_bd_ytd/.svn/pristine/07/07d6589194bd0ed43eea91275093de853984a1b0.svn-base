//判断输入项是否合法
function checkNull(){
	if($("#provinceQY").val() == "0"){
		alert("请选择交易地址");
		return false;
	}
	var province = $("#provinceQY").val();
	if(province!="990000"){
		if($("#cityQY").val() == "0"){
			alert("请选择交易地址");
			return false;
		}
		if($("#countryQY").val() == "0"){
			alert("请选择交易地址");
			return false;
		}
	}
	if(province!="820000"){
		if($("#cityQY").val() == "0"){
			alert("请选择交易地址");
			return false;
		}
		if($("#countryQY").val() == "0"){
			alert("请选择交易地址");
			return false;
		}
	}
	if($("#HKRQ").val().length == 0){
		alert("请选择汇款日期");
		$("#HKRQ").focus();
		return false;
	}
	if($("#HKR").val().length == 0){
		alert("请输入汇款人姓名");
		$("#HKR").focus();
		return false;
	}
	if($("#JYLSH").val().length == 0){
		alert("请输入交易流水号");
		$("#JYLSH").focus();
		return false;
	}
	if($("#JYYH").val().length == 0){
		alert("请输入交易行名");
		$("#JYYH").focus();
		return false;
	}
	return true;
}
function saveJydInfo(){
	var flag = checkNull();
	if(flag){
		//修改注册信息
	 	var XmlData = {};
	 	XmlData["HKRQ"]=$("#HKRQ").val();
	 	XmlData["HKR"]=$("#HKR").val();
	 	XmlData["JYLSH"]=$("#JYLSH").val();
	 	XmlData["JYYH"]=$("#JYYH").val();
	 	XmlData["HKBZ"]=$("#HKBZ").val();
	 	XmlData["orderIds"]=orderIds;
	 	
	 	//拼接地址
	 	var ZCXX07="";  //地址编码
	   	var ZCXX08="";  //中文地址
	   	if($("#provinceQY").val() != "0"){
		  	ZCXX07=$("#provinceQY").val();
		  	ZCXX08=ZCXX08+$("#provinceQY option:selected").text();
	   	}
	   	if($("#cityQY").val() != "0"){
		  	ZCXX07=$("#cityQY").val();
		  	ZCXX08=ZCXX08+$("#cityQY option:selected").text();
	   	}
	   	if($("#countryQY").val() != "0"){
	      	ZCXX07=$("#countryQY").val();
		  	ZCXX08=ZCXX08+$("#countryQY option:selected").text();
	   	}
	   //	ZCXX08=ZCXX08+$("#QYdzdetail").val();
	   	
	   	XmlData["JYDZ"]=ZCXX08;

	   	//与数据库交互
	   	var url="/OrderFlowManage/updateJydInfo.action";
	   	ajaxQ({
		"url" : encodeURI(url),
		"data":{"XmlData":escape(JSON.stringify(XmlData))},
		"callback" : function(data){
					var data1=data.state;
					if(data1=="success"){
						//alert("转账信息提交成功!");
						if(confirm("转账信息提交成功!")){
							window.location.href="/customer/qfy/back/shop-main.html?menuId=A303";
