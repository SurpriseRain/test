//点击立即支付
function payOnline(){
	//支付方式选择
	var skfsArr=[];
	$(".ljzf").attr("disabled",false);
	$(".ljzf").on("click",function(){
	    $(".pay_main").find("input[name='payment']").each(function(index){
		    if($(this).is(":checked")){
				var json={};
				var SKLIST=[];
				json.ZHBJ=2;
				json.JYDH=JYDH;
				var jsonItem={};
				jsonItem.SKFS=$(this).val();//收款方式:支付宝支付6  银联支付8，微信支付9，ebc支付
				jsonItem.ZFJE=zfje;
				SKLIST.push(jsonItem);
				json.SKLIST=SKLIST;
				skfsArr.push(json);
				//savePayment(json); -- 换成从后台调用
			 }
		});
	    var payment=$("input[name='payment']:checked").val();
		if(payment==undefined){
		  alert("请选择支付方式！");
		  return;
		}
		$(".ljzf").attr("disabled",true);
		var xmlData=[];
		var json={};
		json.payment=payment;//支付类型
		json.JYDH=JYDH;//订单号
		json.AMOUNT=zfje;//订单总金额
		json.ZTID=ztid;//主体公司
		json.HBID=user.ZCXX01;//伙伴
		json.LX=user.LX;//用户类型
		/**物流信息**/
		json.logistics_type="EXPRESS";//物流类型
		json.logistics_fee="0.00";//物流费用
		json.logistics_payment="SELLER_PAY";//物流支付方式
		/**收货信息**/
		//var addressList = queryADRESS();
		//json.WIDreceive_name=addressList[0].SHRMC;//"张三";//收货人姓名
		//json.WIDreceive_address=addressList[0].ADDRESS;//"北京市海淀区";//收货人地址
		//json.WIDreceive_zip=addressList[0].YZBM;//"123456";//收货人邮编
		//json.WIDreceive_phone=addressList[0].GDDH;//"0571-88158090";//收货人电话号码(固定电话)
		//json.WIDreceive_mobile=addressList[0].MOBILE;//"13312341234";//收货人手机号码
		xmlData.push(json);
		//window.location.href="/customer/qfy/shopping/ABCWait.html?r="+Math.random()+"&xsdd01="+JYDH+"&ztid="+ztid;
		if(payment == "6"){
		    //淘宝支付
			window.open("/AlipayOnline/payOnline.action?r="+Math.random()+"&XmlData="+JSON.stringify(xmlData)+"&skfsArr="+JSON.stringify(skfsArr),"_self");
		}else if(payment == "8"){
			//银联支付 -- 收款方式未加
			//modify 2015.11.18. 修改银联支付的处理方法与支付宝的处理相同
			window.open("/UnionpayOnline/payOnline.action?r="+Math.random()+"&XmlData="+JSON.stringify(xmlData)+"&skfsArr="+JSON.stringify(skfsArr),"_self");
			/*
			var url="/OrderFlowManage/insertOrderPayway.action?XmlData="+JSON.stringify(skfsArr);
			ajaxQ({
				"url" : url,
				"callback" : function(returnVal){
	    			if(returnVal.STATE=="1"){
	    				var url = "/customer/qfy/shopping/unionpaySend.jsp?r="+Math.random()+"&OrderAmount="+zfje+"&JYDH="+JYDH;
	    				window.open(url,"_self");
	    			}else if(returnVal.STATE=="0"){
	    				alert(returnVal.ERROR);
	    			}
				}
			});*/
		}else if(payment == "7"){
			//铜根源支付
			var jsonTgy={};
			jsonTgy.ORDER_NO=JYDH;//订单号
			window.open("http://192.168.1.110:8088/TGYInterface/TradeDate.action?r="+Math.random()+"&json="+JSON.stringify(jsonTgy),"_self");
		}else if(payment == "9"){
			window.open("/customer/qfy/weixin/weixin_scan_pay.html?r="+Math.random()+"&XmlData="+JSON.stringify(xmlData)+"&skfsArr="+JSON.stringify(skfsArr)+"&JYDH="+JYDH+"&zfje="+zfje,"_self");
		}else if(payment == "11"){
			var otherParams = "";
			if(user.LX==44){
				if(!checkInfo('#YHKH','银行卡号不能为空'))
					return false;
				if(!checkInfo('#YHMC','用户名称不能为空'))
					return false;
				if(!checkInfo('#WYKHH','网银客户号不能为空'))
					return false;

				otherParams="&yhkh="+$.trim($('#YHKH').val())+"&yhmc="+$.trim($('#YHMC').val())+"&wykhh="+$.trim($('#WYKHH').val());
			}else{
				$(".con1 h3 em").css('display','none'); 
			} 
			//ebc钱包支付
			window.open("/EbcController/payOnline.action?r="+Math.random()+"&XmlData="+JSON.stringify(xmlData)+"&skfsArr="+JSON.stringify(skfsArr)+otherParams,"_self");
		}
	});
}

//保存收款方式
function savePayment(json){
	var xmlData=[];
	xmlData.push(json);
    var url="/OrderFlowManage/insertOrderPayway.action?XmlData="+JSON.stringify(xmlData);
	ajaxQ({
		"url" : url,
		"callback" : function(returnVal){
			var returnVal=visitService(url);
	    	if(returnVal.STATE=='1'){
	    		return returnVal;
	    	}else if(returnVal.STATE=='0'){
	    		alert(returnVal.ERROR);
	    		return false;
	    	}
		}
	});
}

