//搜索功能
function selectSrch(usercookie){
 	$("#search_btn").on("click",function(){
	 	if($("#tjbh").val()=="cp") {
		 	if($("#sousuo").val()==null || $("#sousuo").val()=='') {
				alert("请输入查询内容");
				return;
			}
	 		if($("#sousuo").val()!=rmssc) {
		 		var spxx04=($("#sousuo").val()).trim();
		 		location.href="/gui_o2o/sydjt/fore/member_shop.html?SPXX04="+escape(spxx04);
	 		} else {
		 		location.href="/gui_o2o/sydjt/fore/member_shop.html?SPXX04="+escape(rmssc);
	 		}
		} else if($("#tjbh").val()=="dp") {
		 	if($("#sousuo").val()==null || $("#sousuo").val()=='') {
				alert("请输入查询内容");
				return;
			}
			var dpmc = ($("#sousuo").val()).trim();
			location.href="/gui_o2o/sydjt/fore/member_shop.html?SPXX04="+escape(dpmc);
		} else if($("#tjbh").val()=="tm") {
				$("#nxsou").find("li").remove();
				var tm = ($("#sousuo").val()).trim();
				var XmlData = [{"BARCODE":tm}];
		 		var srchUrl=baseUrl+"/MyGoods/SrchGoods.action?XmlData="+JSON.stringify(XmlData);
		 		var rData=visitService(srchUrl);
		 		if(rData.STATE=="1"){
		 			if(rData.goodsMap.JL_State!="接收失败" && rData.ino2o==0){
		 				var goodsMap=rData.goodsMap;
		 				var itemName = goodsMap.itemName;
		 				var itemCode = tm;
						location.href="/gui_o2o/sydjt/fore/member_shop.html?itemCode="+itemCode+"&itemName="+escape(itemName);
		 			} else if(rData.goodsMap==null && rData.ino2o==1) {
		 				location.href="/gui_o2o/sydjt/fore/product.html?spxx01="+rData.spxx.SPXX01+"&zcxx01="+rData.spxx.ZCXX01+"&gsmc="+escape(rData.spxx.ZCXX02);
		 			}else{
		 				alert("查询不到符合条件的商品!");
		 				return;
		 			}
		 		}else{
		 			$("#nxsou").removeClass("lx");
	 				alert("查询异常!");
		 			return;
		 		}			
		 		
		} else if ($("#tjbh").val()=="bsm") {
	 		//搜索企业识别码
	 		$("#nxsou").find("li").remove();
			var bsm = ($("#sousuo").val()).trim();
			var XmlData = [{"BARCODE":bsm}];
	 		var srchUrl=baseUrl+"/QFYCODE/selectQFYCODE.action?XmlData="+JSON.stringify(XmlData);
	 		var returnVal=visitService(srchUrl);
	 		
	 		if(returnVal.list.length>0 ) {
	 			var returnVallist = returnVal.list[0];
	 			alert(returnVallist.firm_name);
	 		}
	 		
		}
 	});
 	
 	$("#head").on("mouseleave",function(){
 		$("#nxsou").hide();
 		$("#nxsou").empty();
 	});
 	$("#sousuo").on("focus",function(){
 		selectFeeling();
 	});
 	$("#sousuo").on("keyup",function(){
 		selectFeeling();
 	});
}

//搜索触发事件公用方法
function selectFeeling() {
		if($("#tjbh").val()=="cp") {
			var inputText=$("#sousuo").val().trim();
		if(inputText==null||inputText==''){
			return false ;
		}
		$("#nxsou").find("li").remove();
		var srchXmlData=[];
		var srchJson={};
	 	if(usercookie==null){
	 		srchJson.GSID="";
		 	srchJson.USERQY="";
	 	}else{
			srchJson.GSID=usercookie.ZCXX01;
			srchJson.USERQY="";
			 		
	 	}
		srchJson.isSpQyCodeLength="0";
		srchJson.isFlppqx="0";
		srchJson.SPNAMELIST=($("#sousuo").val()).trim();
		srchXmlData.push(srchJson);
 		var srchUrl="/PCdisplay/jlquery/selectLenovoSrch.action?XmlData="+JSON.stringify(srchXmlData);
 		var returnVal=visitService(srchUrl);
		if(returnVal.length>0){
	 		$("#nxsou").show();
			$("#nxsou").addClass("lx");
			$(returnVal).each(function(index,srchInfo){
				if(index < 10) {
					$("#nxsou").append("<li><a href=/gui_o2o/sydjt/fore/member_shop.html?SPXX04="+escape(srchInfo.SPNAME)+"></a><span></span></li>");
					$("#nxsou").find("li").eq(index).find("a").html(srchInfo.SPNAME).on("click",function(){
	 				 	var spname=$(this).html();
	 				 	$("#sousuo").val(spname);
	 				 	$(this).parents("ul").removeClass("lx").find("li").remove();	 				 	
					});
					$(".logo_search").find("ul li").eq(index).find("span").html();
				}
			});
		}else{
			$("#nxsou").removeClass("lx");
		}
	} else if($("#tjbh").val()=="dp") {
		$("#nxsou").find("li").remove();
		var dpmc = ($("#sousuo").val()).trim();
		var XmlData = [{"dpmc":dpmc}];
 		var srchUrl=baseUrl+"/QFY/selectDPSrch.action?XmlData="+JSON.stringify(XmlData);
 		var returnVal=visitService(srchUrl);
 		var returnVallist = returnVal.list;
 		if(returnVallist.length>0){
	 		$("#nxsou").show();
			$("#nxsou").addClass("lx");
			$(returnVallist).each(function(index,srchInfo){
				if(index < 10) {
					$("#nxsou").append("<li><a href=/gui_o2o/sydjt/fore/qfy_shop.html?gsid="+srchInfo.ZCXX01+"&gsmc="+escape(srchInfo.ZCXX02)+"></a><span></span></li>");
					$("#nxsou").find("li").eq(index).find("a").html(srchInfo.ZCXX02).on("click",function(){
	 				 	var spname=$(this).html();
	 				 	$("#sousuo").val(spname);
	 				 	$(this).parents("ul").removeClass("lx").find("li").remove();	 				 	
					});
					$(".logo_search").find("ul li").eq(index).find("span").html();
				}
			});
		}else{
			$("#nxsou").removeClass("lx");
		}	 		
	}
}

