function loadFloorGoods(){
	$(returnData.mapList).each(function(index,lcJson){
		var flJson = $(".main-left").find("div.folor").eq(index);
		flJson.find("h2").html(lcJson.LCFL02);
		//加载楼层分类
		//$(lcJson.fllx).each(function(j,lxJson){
		//	flJson.find("div.zcd").append("<a href='#'>"+lxJson.SPFL02+"</a><span></span>");
		//});
		flJson.find("div.zcd").append("<a href='/customer/sydjt/showMessage/gyl-index.html'>更多</a>");
		var spObj = flJson.find("ul.pro-case");
		//加载楼层商品和图片
		
		$(lcJson.lcspfl).each(function(i,spflJson){
			$(spflJson.lcsp).each(function(j,lcsp){
				var lcspUrl="";
				if(lcsp.SPTP02==null){//为null时候后面商品不显示了.
					lcspUrl="/customer/sydjt/images/msimg.jpg";
					}else{
					//楼层商品图片路径
					lcspUrl=imgUrl+lcsp.ZCXX01+"/"+lcsp.SPXX02+"/images/"+lcsp.SPXX02+"_1_big."+(lcsp.SPTP02).split(".")[1];
						}
				//商品图片和链接
				spObj.find("li").eq(j).find("a").attr("href","/customer/sydjt/showProduct/product.html?spxx01="+lcsp.SPXX01+"&zcxx01="+lcsp.ZCXX01+"&gsid="+lcsp.ZCXX01+"&gsmc="+escape(lcsp.ZCXX02)).find("img").addClass("lazy").attr("data-original",lcspUrl).attr("src","../../images/s.gif").attr("onerror","this.onerror=null;this.src='/customer/sydjt/images/msimg.jpg'").attr("title",lcsp.SPMC);
				//商品名称和链接
				spObj.find("li").eq(j).find("div.pro-name").find("a").attr("href","/customer/sydjt/showProduct/product.html?spxx01="+lcsp.SPXX01+"&zcxx01="+lcsp.ZCXX01+"&gsid="+lcsp.ZCXX01+"&gsmc="+escape(lcsp.ZCXX02)).html(lcsp.SPMC);

				if(usercookie!=null) {
					if((usercookie.LX=="43"&&usercookie.ZCXX01!=lcsp.ZCXX01)||usercookie.ZCGS01!="4"){
						spObj.find("li").eq(j).find("div.pro-pic").eq(0).find("b").html("价格不可见");
						}else{
						//商品零售价SPJGB02零售价、SPJGB05分销价
						spObj.find("li").eq(j).find("div.pro-pic").eq(0).find("b").html(accounting.formatMoney(lcsp.SPJGB05));
						if(usercookie.LX=="24")spObj.find("li").eq(j).find("div.pro-pic").eq(0).find("b").html(accounting.formatMoney(lcsp.SPJGB02));
							}
				} else {
						//spObj.find("li").eq(j).find("div.pro-pic").eq(0).find("b").html("登录可见");未登录显示零售价
						spObj.find("li").eq(j).find("div.pro-pic").eq(0).find("b").html(accounting.formatMoney(lcsp.SPJGB02));						
				}
				//购买链接
				spObj.find("li").eq(j).find("div.pro-rz").eq(0).find("a.cat").attr("href","/customer/sydjt/showProduct/product.html?spxx01="+lcsp.SPXX01+"&zcxx01="+lcsp.ZCXX01+"&gsid="+lcsp.ZCXX01+"&gsmc="+escape(lcsp.ZCXX02));
				//店铺名称和链接
				spObj.find("li").eq(j).find("div.pro-dp").find("a.dp").attr("href","/customer/sydjt/showDpxx/shopAllGoods.html?gsid="+lcsp.ZCXX01+"&gsmc="+escape(lcsp.ZCXX02)).html(lcsp.ZCXX02);

			});
		});
		
	});
	$('.main-left img.lazy').lazyload({
        event : "scroll",
        effect : "fadeIn",
        failure_limit : 10
        //threshold : -100
    });
}

