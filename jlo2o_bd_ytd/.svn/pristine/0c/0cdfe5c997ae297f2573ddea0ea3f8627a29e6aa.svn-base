}

// 为空判断
function checkNull(){
	var ppb02 = $("#ppbNAME").val();
	if(ppb02 == "" || ppb02 == null){
		tse("ppxzts","请选择品牌!");
		return  false;
	}
	var sptma=$("#BARCODE").val();
	if(sptma == "" || sptma == null){
		tse("sptmts","请输入商品条码!");
		return false;
	} else if(sptma.length != 13){
		tse("sptmts","请输入13位商品条码!")
		return false;
	}
	var scqyzbm = $("#SPGL24").val();
	if(scqyzbm == "" || scqyzbm == null){
		tse("qybmts","请输入生产企业编码!");
		return false;
	}
	var zjcljh = $(".styles_-1").val();
	if(zjcljh == "" || zjcljh == null){
		tse("ljhts","请输入主机厂零件号!");
		return false;
	}
	var spmca = $("#SPXX04").val();
	if(spmca == "" || spmca == null){
		tse("spmcts","请输入商品名称!");
		return false;
	}
	var spjq = $("#SPJGB05").val();
	if(spjq == "" || spjq == null){
		tse("lsjgts","请输入商品零售价!");
		return false;
	}
	if(!/^\d{1,10}(\.\d{1,2})?$/.test(spjq)){
		tse("lsjgts","价格输入错误,最长10位且最多保留两位小数!");
		return false;
	}
	var chang = $("#SPXX09").val();
	if(chang == "" || chang == null){
		tse("lengthts","请输入商品的长度!");
		return false;
	}
	var kuan = $("#SPXX10").val();
	if(kuan == "" || kuan == null){
		tse("widthts","请输入商品的宽度!");
		return false;
	}
	var gao = $("#SPXX11").val();
	if(gao == "" || gao == null){
		tse("heightts","请输入商品的高度!");
		return false;
	}
	var zl = $("#SPXX12").val();
	if(zl == "" || zl == null){
		tse("weightts","请输入商品的重量!");
		return false;
	}
	var ggxha = $("#GGXH").val();
	if(ggxha == "" || ggxha == null){
		tse("ggxhts","请输入商品的规格型号!");
		return false;
	}
	var jldw = $(".ge").val();
	if(jldw == "" || jldw == null){
		tse("jldwts","请选择商品的计量单位!");
		return false;
	}
	var sycx=$(".cx").val();
	if(sycx=="" ||sycx==null){
		tse("sycxts","请选择商品的试用车系!")
		return false;
	}
	// 如果是特定车系 判断是否选择了已选车系
	if(sycx == 1){
		if($("#yxcs").val().length == 0){
			tse("yxcxts","请选择已选车系!")
			return false;
		}
	}
	return true;
}

