@charset "utf-8";
/* CSS 定制的公共样式 */
body{ margin:0 auto; font-size:12px; color:#666; font-family:Arial,Verdana;}
ul,li,h2,h3,h4,input{ list-style:none; margin:0px; padding:0px;}
a{ text-decoration:none; cursor:pointer;}
.clear{ clear:both;}
*{margin:0;padding:0;list-style-type:none;}
a,img{border:0;}
body{font:12px/180% Arial, Helvetica, sans-serif, "新宋体";}
h1, h2, h3, h4, h5, h6{ font-weight:normal; font-size:12px;}
/****************订单****************/

#list-main{width:800px; float:left; background:#FFF; border:#eee 1px solid;}
#list-main .dqlc{ border-bottom:1px solid #e4e4e4; line-height:38px; padding-left:10px; color:#ccc;}
#list-main .dqlc a.dqlc-a{ color:#2254e4;}
#list-main .dqlc b.dqlc-b{ color:#3c3c3c; font-size:13px;}
#list-main .dqlc label.cx{ float:left; font-size:12px; line-height:28px; color:#333; margin:5px;}
#list-main .dqlc input.sr{float:left;line-height:25px; margin-top:5px; width:100px;}
#list-main .dqlc input.ss{float:left;line-height:25px; margin-top:5px; margin-left:180px;}
#list-main .dqlc input.bot{ float:left; border:1px solid #e4e4e4; height:30px; width:60px; margin:5px 20px 0 0px;}
#list-main .dqlc span.ddh{ float:left; color:#666; margin-right:30px; margin-left:10px;}
#list-main .dqlc span.zt,#list-main .dqlc span.dp{ float:left; color:#03F}
#list-main .dqlc span.dp{margin-left:20px;}
#list-main .dqlc span.tha{float:left; width:90%; color:#999; margin-left:10px;}
#list-main table.sellpro{ width:100%; border-collapse: collapse; border:1px solid #e4e4e4; float:left; border-bottom:1px solid #e4e4e4; margin-bottom:10px;}
#list-main table.sellpro tr{ border-bottom:1px solid #eee; height:80px;}
#list-main table.sellpro tr:hover{ background-color:#f9f9f9;}
#list-main table.sellpro tr.title{ margin:0px; padding:0px; height:35px; line-height:35px; width:798px; text-align:left; font-size:14px; font-family:"microsoft yahei"; background-color:#f5f5f5; margin-left:-10px; color:#333; border-bottom:1px solid #e4e4e4; font-weight:bold;}
#list-main table.sellpro tr.title:hover{background-color:#f5f5f5;}
#list-main table.sellpro tr td a{ float:left; color:#3c3c3c; cursor:pointer;}
#list-main table.sellpro tr td a.sq{ float:left; border:#999 1px solid; width:100px; text-align:center; line-height:25px; margin-left:15px;}
#list-main table.sellpro tr td a.sh{ float:left; border:#999 1px solid; width:100px; text-align:center; line-height:25px; color:#CCC;cursor:default;margin-left:15px;}
#list-main table.sellpro tr td input.checkbox{ float:left; margin:15px 5px;}
#list-main table.sellpro tr td select{ float:left; margin-left:25px;}
#list-main table.sellpro tr td.nm{ float:left; width:280px;}
#list-main table.sellpro tr td a.name{ line-height:20px; margin:0 20px; width:250px; height:40px;}
#list-main table.sellpro tr td span.dj{ float:left; margin:5px 15px 0 20px;}
#list-main table.sellpro tr td span.sl{ float:left; margin:5px;}
#list-main table.sellpro tr td a.name:hover{ color:#2f80eb; text-decoration:underline;}
#list-main table.sellpro tr td a.img img{ margin:5px 0px 5px 35px;}
#list-main table.sellpro tr td.img{ width:550px;}
#list-main table.sellpro tr td.img a span.th-yy{ float:left; overflow: hidden; text-overflow: ellipsis; white-space: nowrap; width:80px; margin:30px 10px;; }
#list-main table.sellpro tr td.img1{ width:445px;}
#list-main table.sellpro tr td.img img{ width:50px; height:50px; margin:15px 0px 5px 20px;; border:1px solid #ccc; float:left;}
#list-main table.sellpro tr td a.pic{ font-size:18px; font-family:"microsoft yahei"; color:#e40000; width:80px; margin-left:20px;}
#list-main table.sellpro tr td a.pic font{ font-size:12px;}
#list-main table.sellpro tr td a.bot{ margin-left:5px; padding:5px 5px; color:#2f80eb; text-align:center; width:100px;}
#list-main table.sellpro tr td a.xq{ margin-left:5px; padding:5px 5px; color:#2f80eb; text-align:center;}
#list-main table.sellpro tr td a.hong{ float:left; background:#F03; text-align:center; width:70px; line-height:20px; border-radius:10px; color:#FFF; margin-left:20px;} 
#list-main table.sellpro tr td a.qrsh{ width:80px; height:30px; background-color:#2f80eb; color:#fff; display:block; text-align:center; line-height:30px; font-family:"microsoft yahei"; border-radius:10px;margin-left:15px;}



#pro-fy{ width:770px; height:40px; margin:15px auto; font-size:14px; float:left;}
#pro-fy .fy{ width:700px; margin:0 auto;}
#pro-fy .fy a{ float:left; height:38px; line-height:38px; border:1px solid #ccc; padding:0 12px; margin-left:-1px; color:#004098; text-decoration:none;}
#pro-fy .fy a.xuan{ background-color:#e9e9e9; color:#999;}
#pro-fy .fy a:hover{ background-color:#e9e9e9; text-decoration:underline;}
#pro-fy .fy a.xuan:hover{ background-color:#e9e9e9; color:#999; text-decoration:none;}

#pro-fy .fy span{ float:left; line-height:38px; margin:0 2px 0 20px;}
#pro-fy .fy span.tb{ margin-left:2px;}
#pro-fy .fy input{ float:left; width:40px; height:18px; border:1px solid #ccc; margin:8px 2px 0 2px;}
#pro-fy .fy a.botton{ height:20px; width:30px; background-color:#ccc; margin:8px 0 0 20px; line-height:20px; text-align:center;}


/****************等待处理****************/
#main ul.userName{ width:700px; background-color:#fff; float:left; padding:10px 20px; margin-top:90px;}
#main ul.userName li{ float:left; width:100%; margin-bottom:20px; position:relative;}
#main ul.userName li img.gantan{ float:left; margin-left:100px; margin-top:45px;}
#main ul.userName li label.dd{ float:left; font-size:16px; font-weight:bold; color:#222; margin-top:60px;}
#main ul.userName li.shenhe{ height:200px; padding-left:150px;}
#main ul.userName li.shenhe .shenhe_ts{ font-size:12px; color:#666; font-weight:500;}
#main ul.userName li.shenhe .shenhe_ts a{ color:#ff8800; margin-left:10px;}
#main ul.userName li.shenhe .shenhe_wh{ float:left; width:410px; height:30px; line-height:30px; font-weight:bold; color:#ff8800; font-size:15px;}
#main ul.userName li.shenhe .shenhe_ts{ font-size:12px; color:#666; font-weight:500;}


/*****************卖家审核*************/
ul.tuidan{ width:720px; background-color:#fff; float:left; padding:10px 40px;box-shadow: 0px 0px 8px #ccc; margin-top:30px; margin-bottom:20px;}
ul.tuidan li{ float:left; width:100%; margin-bottom:20px;}
ul.tuidan li h3{ float:left; margin-left:30px; font-family:"microsoft yahei"; color:#3c3c3c; font-size:14px; margin-top:20px;}
ul.tuidan li span{ font-family:"microsoft yahei"; font-size:14px; color:#999; float:left; width:100px; text-align:right; line-height:37px;}
ul.tuidan li input.h{ width:40px; height:30px; color:#e40000; border:1px solid #e4e4e4; text-align:center; float:left; margin:6px 5px 0; font-size:20px;}
ul.tuidan li b{ float:left; width:20px; height:20px; text-align:center; color:#666; font-size:16px; background-color:#e1e1e1; line-height:20px; margin-top:12px;}

ul.tuidan li input.text{ float:left; width:300px; height:35px; border:1px solid #ccc; padding:0 5px; width:250px;}
ul.tuidan li .bt{ float:left; line-height:45px; width:20px; left:150px; color:#cc4400; font-size:20px; text-align:center;}
ul.tuidan li select{ float:left; height:37px; border:1px solid #ccc; padding:0 0 0 5px;}
ul.tuidan li input.radio{ float:left; width:15px; height:15px; margin:12px 5px 13px 0; cursor:pointer;}
ul.tuidan li input.radio1{ cursor:default;}
ul.tuidan li label{ float:left; line-height:37px; margin-right:20px; font-family:"microsoft yahei"; font-size:14px; color:#3c3c3c; font-weight:bold;}
ul.tuidan li label.hui{ color:#CCC;}
ul.tuidan li textarea{ float:left; min-height:100px; max-height:150px; min-width:500px; max-width:600px; color:#CCC; margin-top:10px;}
ul.tuidan a.ty{ float:left; border:#CCC 1px solid; background:#2f80eb; width:150px; line-height:20px; text-align:center; color:#fff; cursor:pointer; margin-left:40px; padding:5px 10px;}
ul.tuidan a.tj{ float:left; margin-left:200px;}
ul.tuidan a.ty:hover{ background:#00F;}


/*****************不同意退货*************/
.thjg{ float:left; width:800px;;}
.thjg img{ float:left; margin:30px 40px;;}
.thjg span{ float:left; color:#F03; font:"微软雅黑"; font-size:14px; margin-top:40px;}

/*****************退货申请输入页*************/
.thgz{ float:left; width:800px;font-family:"microsoft yahei"; box-shadow: 0px 0px 8px #ccc; padding-top:30px; padding-bottom:30px;}
.thgz label{ float:left; font-weight:bold;font-size:14px; margin-left:50px; line-height:22px; height:140px;}
.thgz span{ float:left; font-size:12px;width:70%; margin-left:20px;line-height:30px; border:#f5f5f5 1px solid;}



