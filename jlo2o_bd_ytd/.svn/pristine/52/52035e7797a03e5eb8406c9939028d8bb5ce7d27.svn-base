#cusService{ width:30px; background-color:#e40000; position:fixed; right:5px; top:205px; text-align:center; padding:10px 0; color:#fff; font-family:"microsoft yahei"; cursor:pointer; z-index:999999;font-size:17px;}

#cusService_main{ width:210px; padding:10px 10px 10px 0; min-height:250px; border:1px solid #e4e4e4; background-color:#fff; border-radius:3px; box-shadow:0px 0px 5px #eee; position:fixed; right:-222px; top:205px; z-index:999999; font-family:"microsoft yahei"; }
#cusService_main .title{ width:200px; height:50px; background:url(/customer/sydjt/images/cusService_title.png); border-bottom:1px solid #e4e4e4; margin:0 0 10px 10px;}
#cusService_main .qq_list{ float:left; width:210px; margin-left:10px;}
#cusService_main .qq_list a{ float:left; width:140px; height:33px; margin:10px 10px 0 37px; border:1px solid #e4e4e4; cursor:pointer;}
#cusService_main .qq_list a:hover{ background-color:#eee;}
#cusService_main .qq_list a img{ float:left; width:33px; height:33px; border:0;}
#cusService_main .qq_list a span{ float:left; height:33px; line-height:33px; margin-left:20px; font-family:"microsoft yahei";}
#cusService_main .tel_list{ width:160px; padding:10px 0 10px 40px; font-size:23px; height:50px; line-height:50px; border-top:1px solid #e4e4e4; margin:20px 0 0 10px; float:left; background:url(/customer/qfy/images/sever_tel.png) no-repeat; background-position:0 10px; font-weight:bold;}