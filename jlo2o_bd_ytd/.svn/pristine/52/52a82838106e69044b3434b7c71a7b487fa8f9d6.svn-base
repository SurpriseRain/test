.defaultTreeTable{
			margin : 0;
			padding : 0;
			border : 0;
}

.checkboxtree{
width:100%;
height:250px;
overflow=auto;
background-color:#f5f5f5;
 border-bottom: 1px solid #DEDEDE;
 border-right: 1px solid #DEDEDE;
 border-left: 1px solid #DEDEDE;

}

.containerTableStyle { overflow : auto;}
.standartTreeRow{	font-family : Verdana, Geneva, Arial, Helvetica, sans-serif; 	font-size : 12px; -moz-user-select: none; }
.selectedTreeRow{ font-family : Verdana, Geneva, Arial, Helvetica, sans-serif; 		font-size : 12px;  -moz-user-select: none;  }
.standartTreeImage{ width:18px; height:18px;  overflow:hidden; border:0; padding:0; margin:0; }
.hiddenRow { width:1px;   overflow:hidden;  }
.dragSpanDiv{ 	font-size : 12px; 	border: thin solid 1 1 1 1; }


TABLE TH{
        font-size: 12px;
	background-color: #3E6197;
	color: #E6FFFF;
	text-align: left;
	border: 1px solid #DEDEDE;
	padding: 2px;
}
td.TD{
	empty-cells: show;
	border: 1px solid #DEDEDE;
        font-size: 12px;
}


/* 操作说明信息 */
div.note table, div.tip table, div.important table, div.caution table, div.warning table {
	width: 100%;
	font-size: 12px;
	border: 1px solid #B0C4DE;
	background-color: #F0F8FF;
	color: #000000;
	/* padding inside table area */
	padding: 0.5em;
	margin-bottom: 0.5em;
	margin-top: 0.5em;
}
